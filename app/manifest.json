{
  "name": "Google News Reader",
  "description": "Read news",
  "version": "1",
  "default_locale": "en",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["javascripts/background.js"]
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [ "unlimitedStorage", "notifications", "http://*/*", "appWindow", "experimental"]
}
